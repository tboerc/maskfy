{
  "name": "@tboerc/maskfy",
  "version": "2.0.3",
  "author": "tboerc",
  "license": "MIT",
  "homepage": "https://github.com/tboerc/maskfy/#readme",
  "description": "Pure javascript mask plugin to use anywhere you like",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:clean": "yarn shx rm -rf ./dist",
