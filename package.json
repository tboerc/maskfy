    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "prettier": "^2.5.1",
    "rollup": "^2.64.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "shx": "^0.3.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tboerc/maskfy.git"
  },
  "bugs": {
    "url": "https://github.com/tboerc/maskfy/issues"
  },
  "keywords": [
    "text",
